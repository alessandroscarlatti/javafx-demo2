<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.layout.HBox?>

<fx:root prefHeight="27.0" prefWidth="200.0" type="HBox" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Todo">
   <children>
      <CheckBox mnemonicParsing="false" />
      <Button mnemonicParsing="false" onAction="#doSomething" prefHeight="25.0" prefWidth="100" text="${controller.text}" />
   </children>
</fx:root>
